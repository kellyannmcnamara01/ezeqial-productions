{
  "require": {
    "anubarak/craft-relabel": "1.3.5",
    "craftcms/cms": "3.4.14",
    "craftcms/contact-form": "2.2.6",
    "craftcms/redactor": "2.6.1",
    "ether/seo": "3.6.3",
    "sebastianlenz/linkfield": "1.0.21",
    "verbb/expanded-singles": "1.1.0",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
